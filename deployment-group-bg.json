{
    "applicationName": "ecs-bluegreen-app",
    "autoRollbackConfiguration": {
       "enabled": true,
       "events": [ "DEPLOYMENT_FAILURE" ]
    },
    "blueGreenDeploymentConfiguration": {
       "deploymentReadyOption": {
          "actionOnTimeout": "CONTINUE_DEPLOYMENT",
          "waitTimeInMinutes": 0
       },
       "terminateBlueInstancesOnDeploymentSuccess": {
          "action": "TERMINATE",
          "terminationWaitTimeInMinutes": 5
       }
    },
    "deploymentGroupName": "ecs-bluegreen-dg",
    "deploymentStyle": {
       "deploymentOption": "WITH_TRAFFIC_CONTROL",
       "deploymentType": "BLUE_GREEN"
    },
    "loadBalancerInfo": {
       "targetGroupPairInfoList": [
         {
           "targetGroups": [
              {
                  "name": "bgprodtarget"
              },
              {
                  "name": "bgtesttarget"
              }
           ],
           "prodTrafficRoute": {
               "listenerArns": [
                   "arn:aws:elasticloadbalancing:us-east-1:580285112064:listener/app/ecs-bg-alb/bf7965b27a2a9e9e/0b2713803feeaf3f"
               ]
           }
         }
       ]
    },
    "serviceRoleArn": "arn:aws:iam::580285112064:role/ecsCodedeployRole",
    "ecsServices": [
        {
            "serviceName": "ecs-service-bluegreen",
            "clusterName": "ecs-bluegreen-cluster"
        }
    ]
 }