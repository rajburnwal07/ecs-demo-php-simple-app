{
    "name": "my-ecs-app-build",
    "source": {
      "type": "GITHUB",
      "location": "https://github.com/rajburnwal07/ecs-demo-php-simple-app.git",
      "buildspec": "buildspec.yml",
      "gitCloneDepth": 1

    },
    "sourceVersion": "main",
    "artifacts": {
      "type": "NO_ARTIFACTS" 
    },
    "environment": {
      "type": "LINUX_CONTAINER",
      "image": "aws/codebuild/amazonlinux2-x86_64-standard:3.0",
      "computeType": "BUILD_GENERAL1_SMALL",
      "privilegedMode": true,
      "environmentVariables": [
        {
          "name": "AWS_REGION",
          "value": "us-east-1"
        },
        {
          "name": "AWS_ACCOUNT_ID",
          "value": "580285112064"
        },
        {
          "name": "IMAGE_REPO_NAME",
          "value": "ecs-demo-app"
        },
        {
          "name": "IMAGE_TAG",
          "value": "v1"
        },
        {
            "name": "CONTAINER_NAME",
            "value": "php-app"
        }              
      ]
    },
    "serviceRole": "arn:aws:iam::580285112064:role/codebuild-service-role",
    "timeoutInMinutes": 60,
    "queuedTimeoutInMinutes": 480,
    "vpcConfig": {
        "securityGroupIds": [
             "sg-f699b1a8"
        ],
        "subnets": [
             "subnet-00a16e01a65505dfc"
        ],
        "vpcId": "vpc-0f0a4c75"
      },    
    "logsConfig": {
      "cloudWatchLogs": {
        "status": "ENABLED"
      },
      "s3Logs": {
        "status": "DISABLED",
        "encryptionDisabled": false
      }    
  }
}  